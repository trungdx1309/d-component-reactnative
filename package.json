{
  "name": "d-component-react-native",
  "version": "0.0.64",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "android": "react-native --port=8088 run-android",
    "android:clean": "cd android && ./gradlew clean && cd .. && react-native --port=8088 run-android",
    "ios": "react-native --port=8088 run-ios --simulator='iPhone 12'",
    "ios:clean": "cd ios && pod install && cd .. && react-native --port=8088 run-ios --simulator='iPhone 12'",
    "ios:iPhone8": "react-native --port=8088 run-ios --simulator='iPhone 8'",
    "iPad": "react-native run-ios --simulator='iPad Pro (9.7 inch)'",
    "start": "react-native start",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --p tsconfig.build.json",
    "build:js": "babel src --out-dir dist --config-file ./src/.babelrc.json --extensions \".ts,.tsx\" --ignore 'src/__test__/*' --source-maps inline",
    "build:lib": "./node_modules/.bin/babel src --out-dir dist --config-file ./src/.babelrc.json --extensions '.ts,.tsx' --ignore 'src/__test__/*'",
    "publishD": "yarn build && yarn publish",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.0.0",
    "moment": "^2.24.0",
    "react-native-device-info": "^8.3.1",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-tab-view": "^3.1.1",
    "tinycolor2": "^1.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.10.1",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.172",
    "@types/react-native": "^0.64.5",
    "@types/react-native-calendars": "1.1264.2",
    "@types/react-native-datepicker": "^1.7.1",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "^16.9.2",
    "@types/tinycolor2": "^1.4.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-typescript-to-proptypes": "^1.3.2",
    "babel-preset-react-native": "*",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react": "17.0.2",
    "react-dom": "^15.4.2",
    "react-native": "0.65.1",
    "react-native-calendars": "1.1266.0",
    "react-native-codegen": "^0.0.7",
    "react-native-date-picker": "^4.1.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "1.10.3",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-modal": "^13.0.0",
    "react-native-pager-view": "^5.4.6",
    "react-native-reanimated": "2.3.0-alpha.2",
    "react-native-router-flux": "^4.2.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.7.2",
    "react-native-vector-icons": "^8.1.0",
    "react-navigation": "^3.13.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": ">=17.0.1",
    "react-native": ">=0.64.1",
    "react-native-gesture-handler": ">=1.9.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
